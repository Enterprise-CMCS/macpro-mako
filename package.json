{
  "name": "mako",
  "version": "0.0.0-managed-by-semantic-release",
  "private": true,
  "bin": {
    "mako": "bin/app.js"
  },
  "scripts": {
    "cdk": "cdk",
    "lint": "eslint",
    "test-tsc": "tsc --skipLibCheck --noEmit",
    "test": "vitest --workspace=./vitest.workspace.ts",
    "bun:dev": "cd react-app && bun run dev",
    "build:react": "cd react-app && vite build",
    "build:lib": "cd lib && tsc",
    "build": "bun run build:lib && bun run build:react"
  },
  "repository": "https://github.com/Enterprise-CMCS/macpro-mako",
  "workspaces": [
    "lib/lambda",
    "lib/libs",
    "lib/local-constructs",
    "lib/packages/*",
    "react-app",
    "bin/cli",
    "mocks",
    "test/e2e"
  ],
  "license": "CC0-1.0",
  "homepage": "https://github.com/Enterprise-CMCS/macpro-mako#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.622.0",
