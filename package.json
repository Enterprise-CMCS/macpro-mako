{
  "name": "mako",
  "version": "0.0.0-managed-by-semantic-release",
  "private": true,
  "bin": {
    "mako": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "build:cli": "turbo build:cli",
    "lint": "eslint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "e2e": "turbo e2e",
    "e2e:ui": "turbo e2e:ui",
    "test-tsc": "tsc --skipLibCheck --noEmit",
    "test": "vitest --project=lib --project=email --project=ui",
    "test:coverage": "bun run test --coverage",
    "test:ui": "bun run test --ui",
    "test:storybook": "cd react-app && bun run test-storybook",
    "cdk:watch": "cdk watch -c stage=${STAGE:-local} --no-rollback",
    "bun:dev": "cd react-app && bun run dev"
  },
  "repository": "https://github.com/Enterprise-CMCS/macpro-mako",
  "workspaces": [
    "lib/lambda",
    "lib/libs",
    "lib/local-constructs",
    "lib/packages/*",
    "react-app",
    "bin/cli",
    "mocks",
    "test"
  ],
  "license": "CC0-1.0",
  "homepage": "https://github.com/Enterprise-CMCS/macpro-mako#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.888.0",
    "@aws-sdk/client-sqs": "^3.888.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@react-email/components": "0.5.3",
    "@types/aws-lambda": "^8.10.152",
    "aws-cdk-lib": "^2.215.0",
    "cdk": "^2.1029.1",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.9",
    "fuse.js": "^7.1.0",
    "react-email": "^3.0.1",
    "react-string-replace": "^1.1.1",
    "source-map-support": "^0.5.21",
    "tsx": "4.20.5"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.14.0",
    "@aws-sdk/client-secrets-manager": "^3.888.0",
    "@aws-sdk/credential-providers": "^3.888.0",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.35.0",
    "@faker-js/faker": "^8.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-vitest": "^9.1.6",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "aws-cdk": "^2.1029.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "happy-dom": "^15.7.4",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.55.0",
    "prettier": "3.6.2",
    "semantic-release": "^21.1.2",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "release": {
    "branches": [
      "production",
      {
        "name": "val",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "packageManager": "bun@1.1.20"
}
