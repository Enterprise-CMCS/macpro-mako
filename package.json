{
  "name": "mako",
  "version": "0.0.0-managed-by-semantic-release",
  "private": true,
  "bin": {
    "mako": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "build:cli": "turbo build:cli",
    "lint": "eslint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "e2e": "turbo e2e",
    "e2e:ui": "turbo e2e:ui",
    "test-tsc": "tsc --skipLibCheck --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "cdk:watch": "cdk watch -c stage=${STAGE:-local} --no-rollback",
    "bun:dev": "cd react-app && bun run dev"
  },
  "repository": "https://github.com/Enterprise-CMCS/macpro-mako",
  "workspaces": [
    "lib/lambda",
    "lib/libs",
    "lib/local-constructs",
    "lib/packages/*",
    "react-app",
    "bin/cli",
    "mocks",
    "test/e2e"
  ],
  "license": "CC0-1.0",
  "homepage": "https://github.com/Enterprise-CMCS/macpro-mako#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.731.1",
    "@aws-sdk/client-sqs": "^3.731.1",
    "@react-email/components": "0.0.25",
    "@types/aws-lambda": "^8.10.147",
    "aws-cdk-lib": "^2.176.0",
    "cdk": "^2.176.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.23.1",
    "react-email": "^3.0.6",
    "source-map-support": "^0.5.21",
    "tsx": "4.15.7"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.5",
    "@aws-sdk/client-secrets-manager": "^3.731.1",
    "@aws-sdk/credential-providers": "^3.733.0",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^8.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.9",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "aws-cdk": "^2.176.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.14.0",
    "happy-dom": "^15.11.7",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.2",
    "semantic-release": "^21.1.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^2.1.8"
  },
  "release": {
    "branches": [
      "production",
      {
        "name": "val",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "packageManager": "bun@1.1.20"
}
